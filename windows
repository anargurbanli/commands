###########################################################################################################################################
---script for search process with port---
:loop
for /F "tokens=1,2,3,4,5" %%A in ('"netstat -ano | findstr 192.168.8.5"') DO (
  echo [%date%, %time%] %%A %%B %%C %%D %%E
  tasklist /fi "pid eq %%E" 
) >> scan.txt
goto loop
###########################################################################################################################################
